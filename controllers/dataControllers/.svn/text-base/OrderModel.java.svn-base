package controllers.dataControllers;

import controllers.dataControllers.Order;
import java.io.Serializable;
import java.util.Vector;

/*
 * To change this template, choose Tools | Templates
 * and open the template in the editor.
 */

 public class OrderModel implements Serializable{
        private Vector<Order>orders;
        private Vector<String> orderNames;

        public OrderModel(){
            orders=new Vector<Order>();
            orderNames=new Vector<String>();
        }


        public Vector<String> getOrderNames() {
            return orderNames;
        }

        public void setOrderNames(Vector<String> orderNames) {
            this.orderNames = orderNames;
        }


        public Vector<Order> getOrders() {
            return orders;
        }

        public void setOrders(Vector<Order> orders) {
            this.orders = orders;
        }
    }